
{
  "ai_assistant": {
    "enabled": true,
    "monitoring_interval": 300,
    "upgrade_interval": 3600,
    "auto_commit": true,
    "auto_deploy": false,
    "model": "gpt-4",
    "max_tokens": 2000,
    "temperature": 0.7
  },
  "optimization_targets": {
    "mining_efficiency": true,
    "error_reduction": true,
    "performance_tuning": true,
    "security_enhancements": true,
    "feature_development": true
  },
  "monitoring": {
    "performance_metrics": true,
    "error_tracking": true,
    "usage_analytics": true,
    "health_checks": true
  },
  "github_integration": {
    "auto_commit": true,
    "commit_message_prefix": "AI Assistant:",
    "branch": "main",
    "backup_before_changes": true
  }
}
